

using System;
class Program
{
    static void Main()
    {
        Console.Write("Enter base: ");
        int baseNum = Convert.ToInt32(Console.ReadLine());

        Console.Write("Enter exponent: ");
        int exponent = Convert.ToInt32(Console.ReadLine());

        int result = 1;
        for (int i = 0; i < exponent; i++)
        {
            result *= baseNum;
        }

        Console.WriteLine($"Result: {result}");
    }
}
